{
  "name": "canvas-interview-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "ws": "^4.1.0"
  },
  "scripts": {
    "start": "nodemon -w public -w src -w server.js ./server.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "bracketSpacing": false,
          "jsxBracketSameLine": true
        }
      ]
    }
  },
  "devDependencies": {
    "nodemon": "^1.17.1"
  }
}
